<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dpmall.db.dao.BlacklistDao">
	<resultMap id="productStatisticResultMap" type="com.dpmall.db.bean.BlacklistEntity">
	    <result property="pk" column="PK" />
	    <result property="customer" column="P_customer" />
	</resultMap>
	
	<select id="getByPK"  resultMap="productStatisticResultMap">
		select 
			b.PK , b.p_customer
		from blacklist b where b.PK=pk 
	 
	</select>
	
	
</mapper>